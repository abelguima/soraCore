                        @CATKIN_DEVEL_PREFIX@/include/rapidDds
                        @CATKIN_DEVEL_PREFIX@/include/rapidExtDds
                        @CATKIN_DEVEL_PREFIX@/include/rapidExtArcDds )
                        
## add include dirs to catkin_INCLUDE_DIRS 
set( catkin_INCLUDE_DIRS  ${catkin_INCLUDE_DIRS} ${KNRAPID_INCLUDE_DIR} )